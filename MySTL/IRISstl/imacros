#ifndef _IMACROS_
#define _IMACROS_

#define _IRIS_BEGIN \
	namespace IRIS {
#define _IRIS_END \
	}

#define _IRIS IRIS::

#define _IRIS_CXX17_CONSTEXPR inline
#define _IRIS_CONSEXCEPT const noexcept
#define _IRIS_ADDRSOF(ARG) __builtin_addressof(ARG)

typedef unsigned __int64 isize_t;
typedef __int64 ioffset_t;

#define I_PI 3.14159265358979323846

#endif // _IMACROS_
